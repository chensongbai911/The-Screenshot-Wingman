.page {
  padding: 32rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  min-height: 80vh;
}

.status-card {
  text-align: center;
}

.icon {
  width: 120rpx;
  height: 120rpx;
  margin-bottom: 24rpx;
}

.status-text {
  display: block;
  font-size: 32rpx;
  font-weight: 600;
  margin-bottom: 12rpx;
}

.status-hint {
  display: block;
  color: #86909C;
  font-size: 26rpx;
}
